  </aside>
  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
    </section>
    <!-- Main content -->
    <section class="content">
    <p><i><%= feedback %></i></p>
    <form action='/commit_form' enctype="multipart/form-data" method="post">
    <h3>General Information</h3>
    Name: <input type ="text" name ="user[name]"><br>
    Address: <input type ="text" name ="user[street_address]"><br>
    <!-- PDA Date: <input type ="date" name ="pda_date"><br>
    Date: <input type ="date" name ="date"><br> -->
    Date of Damage: <input type ="date" name ="user[date_damaged]"><br>
    County: <input type ="text" name ="user[county]"><br>
    Municipality: <input type ="text" name ="user[municipality_name]"><br>
    City: <input type ="text" name ="user[city]"><br>
    <!-- Code: <input type ="text" name ="municipality_code"><br> -->
    Zip Code: <input type ="number" name ="user[zip_code]"><br>
    Longitude: <input type ="number" step="any" name ="user[longitude]"><br>
    Latitude: <input type ="number" step="any" name ="user[latitude]"><br>
    Type of Disaster: <input type ="text" name ="user[incident_type]"><br>
    Basement Water Height(inches): <input type="number" name="user[basement_water]"><br>
    First Floor Water Height(inches): <input type="number" name="user[first_floor_water]"><br>
    Location Notes: <textarea rows="4" cols="50" name="user[location_notes]"></textarea><br>
    <h3>Primary Home?</h3>
      <input type="radio" name="user[primary_home]" value="yes">Yes<br>
      <input type="radio" name="user[primary_home]" value="no">No<br>
    <h3>Renter?</h3>
      <input type="radio" name="user[renter]" value="yes">Yes<br>
      <input type="radio" name="user[renter]" value="no">No<br>
    <h3>Flood Insurance?</h3>
      <input type="radio" name="user[flood_insurance]" value="yes">Yes<br>
      <input type="radio" name="user[flood_insurance]" value="no">No<br>
    <h3>Home or Mobile?</h3>
      <input type="radio" name="user[homeormobile]" id="homeormobile" value="home" onclick="homeOrMobile('Home');">Home<br>
      <input type="radio" name="user[homeormobile]" id="homeormobile" value="mobile" onclick="homeOrMobile('Mobile');">Mobile<br>
    <!-- <h3>Market Value of Property</h3>
      <h4 class="tabbed">Structure</h4>
      <input class ="tabbed" type="number" name="user[structure_mv">
      <h4 class="tabbed">Contents</h4>
      <input class ="tabbed" type="number" name="user[contents_mv">
