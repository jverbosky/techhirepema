    <td class="tg-baqh">Mobile <br>Home</td>
  </tr>
  <tr>
    <td class="tg-yw4l">Foundation</td>
    <td class="tg-baqh"><input type="number" size="1" maxlength="1" min="0" max="7" name="user[foundation_home]" value="<%=results["foundation_home"]%>" id="foundation_home" onchange="onHomeChanged('foundation_home');">></td>
    <td class="tg-4luf"></td>
    <td class="tg-baqh">7</td>
    <td class="tg-4luf"></td>
  </tr>
