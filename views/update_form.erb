    <td class="tg-baqh"><input type="number" size="2" maxlength="2" min="0" max="20" name="user[roof_mobile]" value="<%=results["roof_mobile"]%>" id="roof_mobile" onchange="onMobileChanged('roof_mobile');"></td>
    <td class="tg-baqh">9</td>
    <td class="tg-baqh">20</td>
  </tr>
  <tr>
    <td class="tg-yw4l">Interior Walls</td>
    <td class="tg-baqh"><input type="number" size="2" maxlength="2" min="0" max="28" name="user[interior_walls_home]" value="<%=results["interior_walls_home"]%>" id="interior_walls_home" onchange="onHomeChanged('interior_walls_home');"></td>
    <td class="tg-baqh"><input type="number" size="2" maxlength="2" min="0" max="25" name="user[interior_walls_mobile]" value="<%=results["interior_walls_mobile"]%>" id="interior_walls_mobile" onchange="onMobileChanged('interior_walls_mobile');"></td>
    <td class="tg-baqh">28</td>
    <td class="tg-baqh">25</td>
  </tr>
  <tr>
    <td class="tg-yw4l">Plumbing</td>
    <td class="tg-baqh"><input type="number" size="2" maxlength="2" min="0" max="10" name="user[plumbing_home]" value="<%=results["plumbing_home"]%>" id="plumbing_home" onchange="onHomeChanged('plumbing_home');"></td>
    <td class="tg-4luf"></td>
    <td class="tg-baqh">10</td>
    <td class="tg-4luf"></td>
  </tr>
  <tr>
    <td class="tg-yw4l">Heating/A.C.</td>
    <td class="tg-baqh"><input type="number" size="2" maxlength="2" min="0" max="10" name="user[heating_ac_home]" value="<%=results["heating_ac_home"]%>" id="heating_ac_home" onchange="onHomeChanged('heating_ac_home');"></td>
    <td class="tg-4luf"></td>
    <td class="tg-baqh">10</td>
    <td class="tg-4luf"></td>
  </tr>
  <tr>
    <td class="tg-yw4l">Electrical</td>
    <td class="tg-baqh"><input type="number" size="1" maxlength="1" min="0" max="6" name= "electrical_home]" value="<%=results["electrical_home"]%>" id="electrical_home" onchange="onHomeChanged('electrical_home');"></td>
    <td class="tg-4luf"></td>
    <td class="tg-baqh">6</td>
    <td class="tg-4luf"></td>
  </tr>
  <tr>
    <td>Total % Damaged</td>
    <td id="home_total_pct_damaged">0
    <td id="mobile_total_pct_damaged">0
    <td colspan="2">
  </tr>
  </table><br>
  <table class="tg">
    <tr>
      <th class="tg-s6z2" colspan="3">Damage Category</th>
    </tr>
    <tr>
      <td class="tg-943m"></td>
