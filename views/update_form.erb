    </tr>
    <tr>
      <td class="tg-s6z2">Destroyed</td>
      <td class="tg-s6z2"><input type="number" size="2" maxlength="2" min="0" max="28" name="user[destroyed_home]" value="<%=results["destroyed_home"]%>"></td>
      <td class="tg-s6z2"><input type="number" size="2" maxlength="2" min="0" max="28" name="user[destroyed_mobile]" value="<%=results["destroyed_mobile"]%>"></td>
    </tr>
    <tr>
      <td class="tg-baqh">Major</td>
      <td class="tg-baqh"><input type="number" size="2" maxlength="2" min="0" max="28" name="user[major_home]" value="<%=results["major_home"]%>"></td>
      <td class="tg-baqh"><input type="number" size="2" maxlength="2" min="0" max="28" name="user[major_mobile]" value="<%=results["major_mobile"]%>"></td>
