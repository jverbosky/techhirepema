    <td class="tg-baqh"><input type="number" size="2" maxlength="2" min="0" max="20" name="user[floor_frame_mobile]" value="<%=results["floor_frame_mobile"]%>" id="floor_frame_mobile" onchange="onMobileChanged('floor_frame_mobile');"></td>
