      <td class="tg-baqh"><input type="number" size="2" maxlength="2" min="0" max="28" name="user[minor_mobile]" value="<%=results["minor_mobile"]%>"></td>
    </tr>
    <tr>
      <td class="tg-baqh">Affected</td>
      <td class="tg-baqh"><input type="number" size="2" maxlength="2" min="0" max="28" name="user[affected_home]" value="<%=results["affected_home"]%>"></td>
      <td class="tg-baqh"><input type="number" size="2" maxlength="2" min="0" max="28" name="user[affected_mobile]" value="<%=results["affected_mobile"]%>"></td>
    </tr>
    <tr>
      <td class="tg-baqh">Inaccessible</td>
      <td class="tg-baqh"><input type="number" size="2" maxlength="2" min="0" max="28" name="user[inaccessible_home]" value="<%=results["inaccessible_home"]%>"></td>
      <td class="tg-baqh"><input type="number" size="2" maxlength="2" min="0" max="28" name="user[inaccessible_mobile]" value="<%=results["inaccessible_mobile"]%>"></td>
